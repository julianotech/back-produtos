{
  "name": "backend-default",
  "version": "1.0.0",
  "main": "index.js",
  "author": "julianostroschon <julianostroschon@gmail.com>",
  "engines": {
    "node": "20.0.0"
  },
  "scripts": {
    "knex": "ts-node -r dotenv/config ./node_modules/.bin/knex --knexfile ./src/infra/knexfile.ts",
    "build:clean": "rm -rf dist/ && yarn generate && yarn tsc",
    "build": "yarn generate && yarn tsc",
    "start": "yarn build && node dist/index.js",
    "dev": "yarn generate && tsx --watch src/index.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install",
    "db:prepare": "yarn knex migrate:latest",
    "generate": "graphql-codegen"
  },
  "license": "MIT",
  "devDependencies": {
    "@eddeee888/gcg-typescript-resolver-files": "^0.8.1",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@types/bcrypt": "^5.0.2",
    "@types/knex": "^0.16.1",
    "@types/node": "^20.12.12",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.11.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.10.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.23.0",
    "graphql-yoga": "^5.3.1",
    "husky": "^9.0.11",
    "knex": "^3.1.0",
    "pg": "^8.11.5",
    "prettier": "^3.2.5"
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  }
}
